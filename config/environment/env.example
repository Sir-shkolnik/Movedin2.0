# MovedIn 3.0 - Smart & Secure Backend Environment Variables
# Copy this file to .env and fill in your real values

# Database
DATABASE_URL=sqlite:///./movedin3.db

# Email Configuration (from V2.0)
SMTP_SERVER=smtp.office365.com
SMTP_PORT=587
SMTP_USERNAME=support@movedin.com
SMTP_PASSWORD=your_actual_email_password_here

# Stripe Configuration (from V2.0)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Mapbox Configuration (from V2.0)
MAPBOX_ACCESS_TOKEN=pk.your_mapbox_token_here

# App Configuration
APP_NAME=MovedIn 3.0 - Smart & Secure
DEBUG=true
SECRET_KEY=movedin3-smart-secure-key-2025

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:5173,https://movedin-frontend.onrender.com,https://movedin.com,https://www.movedin.com
